#include "stm32f4xx.h"
			

int main(void)
{
	init();
	// Enable RCC clock
	GPIO_InitTypeDef i2sPortSettings;
	GPIO_Struct_Init(&i2sPortSettings);
	i2sPortSettings.GPIO_Mode = GPIO_Mode_AF;
	GPIO_Init(GPIOA, &i2sPortSettings);
	for(;;);
}

void init()
{
	GPIO_InitTypeDef codecPinSettings;
	GPIO_Struct_Init(&codecPinSettings);
	codecPinSettings.GPIO_Mode
	GPIO_Init(GPIO_TypeDef* GPIOx, GPIO_InitTypeDef* GPIO_InitStruc
}
